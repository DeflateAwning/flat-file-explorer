{
	"name": "parquet-explorer",
	"displayName": "Parquet Explorer",
	"description": "Explore Parquet files with SQL.",
	"version": "0.0.1",
	"publisher": "adam-viola",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/adamviola/parquet-explorer"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "parquetExplorer.explorer",
				"displayName": "Parquet",
				"selector": [
					{
						"filenamePattern": "*.parquet"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"lint": "eslint \"src/**/*.ts\""
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"esbuild": "^0.18.15",
		"eslint": "^8.26.0",
		"node-pre-gyp": "^0.17.0",
		"typescript": "^5.1.3"
	},
	"dependencies": {
		"duckdb": "^0.8.1"
	}
}
